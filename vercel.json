{
    "version": 2,
    "builds": [
      {
        "src": "bot.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/static/index.html"
      },
      {
        "src": "/bot",
        "dest": "/bot.py"
      }
    ]
  }
  